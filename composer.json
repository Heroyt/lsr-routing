{
	"name": "lsr/routing",
	"description": "Laser framework core - Routing.",
	"version": "dev-0.2.0",
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.laserliga.cz"
		}
	],
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"phpstan/phpstan": "^1.8",
		"lsr/core": "^0.2-dev",
		"lsr/request": "^0.2-dev"
	},
	"autoload": {
		"psr-4": {
			"Lsr\\Core\\Routing\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Lsr\\Core\\Routing\\Tests\\TestCases\\": "tests/TestCases/",
			"Lsr\\Core\\Routing\\Tests\\Mockup\\": "tests/src/"
		}
	},
	"authors": [
		{
			"name": "Tomáš Vojík",
			"email": "vojik@wboy.cz"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=8.1",
		"lsr/interfaces": "^0.2-dev"
	},
	"extra": {
		"branch-alias": {
			"v0.2": "0.2.x-dev"
		}
	}
}
